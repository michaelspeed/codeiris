    "@prisma/client": "^2.20.1",
    "@sentry/hub": "^6.2.5",
    "@sentry/node": "^6.2.5",
    "@types/react-dom": "^17.0.3",
    "apollo-link-token-refresh": "^0.3.2",
    "apollo-server-express": "^2.22.1",
    "aws-sdk": "^2.884.0",
    "babel-plugin-superjson-next": "^0.2.3",
    "braft-editor": "^2.3.9",
    "clsx": "^1.1.1",
    "cross-env": "^7.0.3",
